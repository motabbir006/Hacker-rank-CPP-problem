#include <iostream>
using namespace std;

int main() {
    int x, y, z, p;
    cin >> x >> y >> z >> p;

    if (x > y && x > z && x > p) {
        cout << x;
    } else if (y > x && y > z && y > p) {
        cout << y;
    } else if (z > x && z > y && z > p) {
        cout << z;
    } else {
        cout << p;
    }

    return 0;
}
