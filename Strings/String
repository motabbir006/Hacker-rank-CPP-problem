#include <iostream>
using namespace std;

int main() {
    string x, y;
    cin >> x >> y;

    int lenx = x.size();
    int leny = y.size();
    cout << lenx << " " << leny << endl;

    string c = x + y;
    cout << c << endl;

    char temp = x[0];
    x[0] = y[0];
    y[0] = temp;

    cout << x << " " << y;

    return 0;
}
